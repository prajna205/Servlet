<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
id="WebApp_ID" version="4.0">
<welcome-file-list>
	<welcome-file>corona_vaccine.html</welcome-file>
	<welcome-file>corona_vaccine.jsp</welcome-file>
</welcome-file-list>

<!-- Servlet configuration -->
<servlet>
<servlet-name>corona</servlet-name>
<servlet-class>com.nt.servlet.VaccineEligibityCheckServlet</servlet-class>
</servlet>

<!-- Servlet mapping -->
<servlet-mapping>
<servlet-name>corona</servlet-name>
<url-pattern>/cvurl</url-pattern>
</servlet-mapping>
  
</web-app>